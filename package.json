{
  "name": "exec-staged",
  "version": "0.0.0",
  "description": "Run commands against the current git index",
  "keywords": [
    "git",
    "lint",
    "staged",
    "lint-staged",
    "pre-commit",
    "commit",
    "hook"
  ],
  "repository": "github:ItsNickBarry/exec-staged",
  "license": "MIT",
  "author": "Nick Barry",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./types": "./dist/types.js"
  },
  "types": "./dist/index.d.ts",
  "bin": {
    "exec-staged": "./dist/bin/cli.js"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "scripts": {
    "build": "pnpm clean && tsc --build",
    "clean": "rm -rf dist/",
    "prepare": "husky",
    "prepublishOnly": "pnpm build",
    "test": "pnpm build && tsx --test --experimental-test-coverage"
  },
  "dependencies": {
    "commander": "^14.0.0",
    "cosmiconfig": "^9.0.0",
    "env-paths": "^3.0.0",
    "execa": "^9.6.0",
    "micromatch": "^4.0.8",
    "on-process-exit": "^1.0.2",
    "semver": "^7.7.2",
    "string-argv": "^0.3.2"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@tsconfig/node22": "^22.0.2",
    "@types/micromatch": "^4.0.9",
    "@types/node": "^22.15.31",
    "@types/semver": "^7.7.0",
    "husky": "^9.1.7",
    "knip": "^5.60.2",
    "lint-staged": "github:ItsNickBarry/lint-staged#knip",
    "prettier": "^3.5.3",
    "prettier-plugin-packagejson": "^2.5.15",
    "tsx": "^4.20.1",
    "typescript": "^5.8.3"
  }
}
